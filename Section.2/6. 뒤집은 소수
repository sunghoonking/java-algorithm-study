package main;

import java.util.*;

class Main {
    public static boolean isPrime(int num) {
        if (num == 1) return false;
        for (int i = 2; i < num; i++) {
            if (num % i == 0) return false;
        }
        return true;

    }

    public static void main(String[] args) {
       
        Scanner sh = new Scanner(System.in);
        int num = sh.nextInt();

        int[] arr = new int[num];
        for (int i = 0; i < arr.length; i++) {
            arr[i] = Integer.parseInt(sh.next());
            int nam = arr[i];
            int re = 0;
            while (nam != 0) {
                int digit = nam % 10;
                re = re * 10 + digit;
                nam /= 10;
//                   뒤집음
            }
            if (isPrime(re))
                System.out.print(re + " ");

        }


    }


}
