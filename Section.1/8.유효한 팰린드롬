package main;

import java.util.*;
class Main {
    public String solution(String str){
        // 출력은 YES OR NO 이기 때문에
        String answer = "NO";
        // 대소문자 구분 없애기 위해서 소문자 통일 그리고 정규표현식과 replaceAll 을 사용해 만약 소문자 a부터 z까지 아니면 빈문자로
        str = str.toUpperCase().replaceAll("[^A-Z]", "");
        // 문자열 tem 으로 만들고 reverse 로 뒤집고 다시 문자열로 만들어줌
        String tem = new StringBuilder(str).reverse().toString();
        // 만약 str 과 tem 내용이 같다면 yes
        if(str.equals(tem)) answer = "YES";
        return answer;
    }

    public static void main(String[] args){
        Main ma = new Main();
        Scanner hon = new Scanner(System.in);
        String str = hon.next();
        System.out.print(ma.solution(str));
    }
}
// 영문자만 체크해서 앞 뒤 뒤집었을 때 같으면 YES 아니면 NO
