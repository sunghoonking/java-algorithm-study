package main;

import java.util.*;
class Main {
    public String solution(int n,String s){
        String answer="";
        for(int i = 0; i <n; i++){
            // 0번 인덱스 부터 7번째 까지 나눔 replace를 사용해서  바꿈
            String put = s.substring(0,7).replace('#','1').replace('*','0');
             // 7부터 끝까지로 변환
            s = s.substring(7);
            // 바꾼 이진수를 int로 변환
            int num = Integer.parseInt(put,2);
            // 아스키 넘버로 전환해서 문자로 변환
            answer +=(char)num;

          


        }

        return answer;
    }

    public static void main(String[] args){
        Main T = new Main();
        Scanner sh = new Scanner(System.in);
        int n = sh.nextInt();
        String str=sh.next();
        System.out.println(T.solution(n,str));
    }
}
