
import java.util.*;

class Main {

    public static void main(String[] args) {
        Scanner sh = new Scanner(System.in);

        int num = sh.nextInt();
        int[] arr = new int[num];
        for (int i = 0; i < num; i++) {
            arr[i] = sh.nextInt();
        }

        int num1 = sh.nextInt();
        int[] arr1 = new int [num1];
        for (int i = 0; i < num1; i++) {
            arr1[i] = sh.nextInt();
        }

        int len = arr.length + arr1.length;

       int[] leng = new int[len];

        System.arraycopy(arr,0,leng,0,arr.length);
        //arr 을 leng 복사
        System.arraycopy(arr1,0,leng,arr.length,arr1.length);
        // arr1을 leng 복사

        Arrays.sort(leng);

        for (int i : leng) {
            System.out.print(i+" ");
        }

    }

}
